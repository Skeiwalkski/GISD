my_ts_imputer <- function(data,outcome_name){
mydata   <- data %>% group_by(Gemeindekennziffer) %>%
select(Gemeindekennziffer,Jahr,"Outcome"=paste(outcome_name)) %>%
mutate(MEAN=mean(Outcome , na.rm=T)) %>% ungroup()
